
class Box {
    private int width;
    private int height;
    private int depth;

    Box(int width, int height, int depth) {
        this.width = width;
        this.height = height;
        this.depth = depth;
    }

    float volume() {
        return width * height * depth;
    }
}

public class Solution1 {
    public static void main(String[] args) {

        Box b1 = new Box(1, 2, 3);
        System.out.println(b1.volume());

    }
}
